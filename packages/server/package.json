{
  "name": "@example/server",
  "description": "GraphQL Server template",
  "private": true,
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "start-dev": "yarn knex/migrate && env-cmd -f .env -- babel-watch src/server.js",
    "start": "yarn knex/migrate && env-cmd -f .env node build/server.js",
    "build": "babel src -d build --extensions \".js\" --no-copy-ignored",
    "knex-migrate": "env-cmd -f .env knex-migrate",
    "knex/migrate": "yarn knex-migrate up",
    "knex/migrate:make": "yarn knex-migrate generate",
    "knex/seed:make": "env-cmd -f .env knex seed:make",
    "knex/seed": "env-cmd -f .env knex seed:run",
    "knex/rollback": "yarn knex-migrate down",
    "knex/rollback:all": "yarn knex-migrate down --to 0"
  },
  "dependencies": {
    "@example/connectors": "*",
    "@example/entities": "*",
    "@example/shared": "*",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "glob": "^7.1.6",
    "graphql": "^15.5.0",
    "graphql-add-middleware": "^0.3.7",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-relay": "^0.6.0",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@example/babel": "*",
    "env-cmd": "^10.1.0"
  }
}
