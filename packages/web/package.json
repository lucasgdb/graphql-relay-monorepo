{
  "name": "@example/web",
  "description": "Web template with GraphQL Relay",
  "private": true,
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "start-dev": "yarn relay && env-cmd -f .env.development webpack serve",
    "relay": "relay-compiler",
    "update": "yarn relay",
    "prebuild": "yarn --frozen-lockfile && rm -rf build && yarn relay",
    "build": "NODE_OPTIONS=--max_old_space_size=8192 env-cmd -f .env.production webpack --mode=production"
  },
  "dependencies": {
    "@date-io/moment": "^2.10.8",
    "@example/ui": "*",
    "@example/shared": "*",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "core-js": "^3.8.3",
    "formik": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-relay": "^10.1.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "relay-config": "^10.1.3",
    "relay-hooks": "^4.1.0",
    "relay-runtime": "^10.1.3",
    "styled-components": "^5.2.1",
    "universal-cookie": "^4.0.4",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@example/babel": "*",
    "@example/webpack": "*",
    "relay-compiler": "^10.1.3"
  }
}
